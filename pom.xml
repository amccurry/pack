	<properties>
		<parcel.assembly.file>src/assemble/parcel.xml</parcel.assembly.file>
		<parcel.version.suffix></parcel.version.suffix>
		<parcel.name>PACK</parcel.name>
		<!--parcel.version>${project.version}${parcel.version.suffix}</parcel.version>-->
		<parcel.version>${maven.build.timestamp}</parcel.version>
		<maven.build.timestamp.format>yy.MM.dd.HH.mm</maven.build.timestamp.format>
	</properties>

	<repositories>
